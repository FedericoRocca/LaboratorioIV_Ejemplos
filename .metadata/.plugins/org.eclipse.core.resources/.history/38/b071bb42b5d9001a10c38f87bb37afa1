package dominio;

public class ArticuloGimnasio extends Articulo
{
    private int peso;

    // Getters/setters
    /**
     * @return the peso
     */
    public int getPeso()
    {
        return peso;
    }

    /**
     * @param peso the peso to set
     */
    public void setPeso(int peso)
    {
        this.peso = peso;
    }
    
    
}
