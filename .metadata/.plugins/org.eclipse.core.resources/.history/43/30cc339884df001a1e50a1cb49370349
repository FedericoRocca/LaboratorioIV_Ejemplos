public class Pajaro
{
    private int Edad;

    /**
     * @return the edad
     */
    public int getEdad()
    {
        return Edad;
    }

    /**
     * @param edad the edad to set
     */
    public void setEdad(int edad)
    {
        Edad = edad;
    }

    public Pajaro(int edad)
    {
        super();
        Edad = edad;
    }

    public Pajaro()
    {
        super();
    }
    
    
}