package dominio;

public class Main
{
    
    public static void main(String[] args)
    {
        Animal animal = new Animal("Animal");
        Perro perro = new Perro("Blue", "PC (Pura calle)");
        Gato gato = new Gato("Simón", "PG (Puro gato)");
        
        System.out.println(animal.toString());
        System.out.println(perro.toString());
        System.out.println(gato.toString());
        
        Animal deforme = new Perro();
        Animal masDeforme = new Gato();
        
        System.out.println(deforme.toString());
        System.out.println(masDeforme.toString());
        
        Animal[] animales = new Animal[3];
        animales[0] = new Animal("Un animal feo");
        animales[1] = new Gato("Gato feo", "Fealdad");
        animales[2] = new Perro("Bob", "Alto perro");
        
        for (Animal animal2 : animales)
        {
            System.out.println(animal2.toString());
        }
    }
    
}
