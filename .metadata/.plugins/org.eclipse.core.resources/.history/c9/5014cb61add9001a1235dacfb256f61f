/**
 * 
 */
package dominio;

/**
 * @author Federico
 *
 */
public class Principal {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		Articulo art = new Articulo(); 
		System.out.println("ID: " + art.getId() + ", nombre: " + art.getNombre() );
		
		art.setId(1);
		art.setNombre("Articulo 1");
		System.out.println("ID: " + art.getId() + ", nombre: " + art.getNombre() );
		
		System.out.println(art.toString());
		
		Articulo art2 = new Articulo("Articulo 2", 2); 
		System.out.println(art2.toString());
		
		Articulo art3 = new Articulo("Articulo 3", 3);
		System.out.println(art3.toString());
	}
}
